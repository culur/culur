{
  "$schema": "node_modules/nx/schemas/nx-schema.json",
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": false
    }
  },
  "targetDefaults": {
    "lint:md": {
      "inputs": [
        "{workspaceRoot}/.markdownlint-cli2.yaml",
        "{projectRoot}/.markdownlint-cli2.yaml",
        "{projectRoot}/**/*.md",
        "!{projectRoot}/coverage/**/*",
        "!{projectRoot}/dist/**/*"
      ],
      "cache": true
    },
    "lint:es": {
      "inputs": [
        "{workspaceRoot}/eslint.config.mjs",
        "{projectRoot}/eslint.config.mjs",
        "{projectRoot}/**/*",
        "!{projectRoot}/coverage/**/*",
        "!{projectRoot}/dist/**/*"
      ],
      "cache": true
    },
    "lint:css": {
      "inputs": [
        "{workspaceRoot}/.stylelintignore",
        "{workspaceRoot}/.stylelintrc.mjs",
        "{projectRoot}/.stylelintignore",
        "{projectRoot}/.stylelintrc.mjs",
        "{projectRoot}/**/*.(vue|css|scss)",
        "!{projectRoot}/coverage/**/*",
        "!{projectRoot}/dist/**/*"
      ],
      "cache": true
    },
    "lint:prettier": {
      "inputs": [
        "{workspaceRoot}/.prettierignore",
        "{workspaceRoot}/.prettierignore.project",
        "{workspaceRoot}/.prettierrc.mjs",
        "{projectRoot}/.prettierignore",
        "{projectRoot}/.prettierrc.mjs",
        "{projectRoot}/**/*",
        "!{projectRoot}/coverage/**/*",
        "!{projectRoot}/dist/**/*"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "build",
        "^build"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    },
    "build": {
      "dependsOn": [
        "gen",
        "^build"
      ],
      "inputs": [
        "{workspaceRoot}/.gitignore",
        "{projectRoot}/**/*",
        "!{projectRoot}/**/*.md",
        "!{projectRoot}/**/*.test.ts"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    }
  },
  "defaultBase": "dev",
  "cacheDirectory": ".nx/cache",
  "plugins": [],
  "nxCloudId": "68196eae76e31b0ad5e98ed1"
}