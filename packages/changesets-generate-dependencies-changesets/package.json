{
  "name": "@culur/changesets-generate-dependencies-changesets",
  "version": "1.2.0",
  "description": "Generate dependencies changesets",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culur/culur.git",
    "directory": "packages/changesets-generate-dependencies-changesets"
  },
  "bugs": {
    "url": "https://github.com/culur/culur/issues"
  },
  "homepage": "https://github.com/culur/culur#readme",
  "author": "culur <culur.net@gmail.com>",
  "license": "MIT",
  "keywords": [
    "culur",
    "action"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "!**/*.json",
    "!**/*.test.*",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "tsup",
    "test": "tsc --noEmit && vitest run"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@octokit/webhooks-types": "^7.5.1",
    "dedent": "^1.5.3",
    "fs-extra": "^11.2.0",
    "minimatch": "^9.0.5"
  },
  "devDependencies": {
    "@culur/config-tsup": "1.1.0",
    "@culur/config-typescript": "1.0.2",
    "@culur/config-vite": "1.0.3",
    "@culur/types": "1.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.10",
    "@vitest/coverage-v8": "^2.0.1",
    "@vitest/ui": "^2.0.1",
    "rimraf": "^6.0.0",
    "tsup": "^8.1.0",
    "type-fest": "^4.21.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.1"
  }
}
