{
  "name": "@culur/config-tsup",
  "version": "0.1.0",
  "description": "Culur's tsup configuration",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culur/culur.git",
    "directory": "packages/config-tsup"
  },
  "bugs": {
    "url": "https://github.com/culur/culur/issues"
  },
  "homepage": "https://github.com/culur/culur#readme",
  "author": "culur <culur.net@gmail.com>",
  "license": "MIT",
  "keywords": [
    "culur",
    "config",
    "typescript"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "tsup",
    "test": "tsc --noEmit && vitest run"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "tsup": "^8.0.2"
  },
  "devDependencies": {
    "@culur/config-typescript": "*",
    "@culur/config-vite": "*",
    "@types/node": "^20.12.12",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
