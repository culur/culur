{
  "name": "@culur/types",
  "version": "1.0.5",
  "description": "Culur's stylelint configuration",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culur/culur.git",
    "directory": "packages/types"
  },
  "bugs": {
    "url": "https://github.com/culur/culur/issues"
  },
  "homepage": "https://github.com/culur/culur/tree/main/packages/types#readme",
  "author": "culur <culur.net@gmail.com>",
  "license": "MIT",
  "keywords": [
    "culur",
    "config"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "!**/*.json",
    "!**/*.test.*",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "tsup",
    "test": "tsc --noEmit && vitest run"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "type-fest": "^4.26.0"
  },
  "devDependencies": {
    "@culur/config-tsup": "1.1.2",
    "@culur/config-typescript": "1.0.3",
    "@culur/config-vite": "1.0.5",
    "@types/node": "^20.14.10",
    "@vitest/coverage-v8": "^2.0.2",
    "@vitest/ui": "^2.0.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  }
}
