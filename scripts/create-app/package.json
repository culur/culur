{
  "name": "@culur/create-app",
  "version": "0.0.0",
  "description": "Culur's create-app script",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culur/culur.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/culur/culur/issues"
  },
  "homepage": "https://github.com/culur/culur#readme",
  "author": "culur <culur.net@gmail.com>",
  "license": "MIT",
  "keywords": [
    "culur",
    "config",
    "stylelint"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    ".stylelintrc.js",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "bin": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup ./src",
    "n10": "tsx src/index ../../../temp/npm-10 --git -o remove -t lib --npm 10",
    "p7": "tsx src/index ../../../temp/pnpm-7 --git -o remove -t lib --pnpm 7",
    "p8": "tsx src/index ../../../temp/pnpm-8 --git -o remove -t lib --pnpm 8",
    "y3": "tsx src/index ../../../temp/yarn-3 --git -o remove -t lib --yarn 3",
    "y4": "tsx src/index ../../../temp/yarn-4 --git -o remove -t lib --yarn 4",
    "dev": "tsx src/index ../../../temp/temp1 -fw vue -b vite --git",
    "test": "vitest run --coverage"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@culur/types": "*",
    "@culur/utils-prompts": "*",
    "@inquirer/prompts": "^5.0.1",
    "@inquirer/type": "^1.3.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cross-spawn": "^7.0.3",
    "dedent": "^1.5.3",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "figures": "^6.1.0",
    "fs-extra": "^11.2.0",
    "hosted-git-info": "^7.0.1",
    "joi": "^17.13.0",
    "latest-version": "^9.0.0",
    "lodash-es": "^4.17.21",
    "normalize-path": "^3.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "simple-git": "^3.24.0",
    "ts-morph": "^22.0.0",
    "type-fest": "^4.16.0"
  },
  "devDependencies": {
    "@culur/config-prettier": "*",
    "@culur/config-tsup": "*",
    "@culur/config-typescript": "*",
    "@culur/config-vite": "*",
    "@types/cross-spawn": "^6.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/hosted-git-info": "^3.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  }
}
